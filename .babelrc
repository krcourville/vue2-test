{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "useBuiltIns": true,
                "targets": {
                    "browsers": [
                        "Chrome >= 52",
                        "FireFox >= 44",
                        "Safari >= 7",
                        "Explorer 11",
                        "last 4 Edge versions"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "plugins": ["transform-runtime"],
    "comments": false
}
